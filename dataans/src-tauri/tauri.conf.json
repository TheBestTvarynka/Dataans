{
  "build": {
    "beforeDevCommand": "trunk serve",
    "beforeBuildCommand": "trunk build",
    "frontendDist": "../dist",
    "devUrl": "http://localhost:1420"
  },
  "bundle": {
    "active": true,
    "targets": "all",
    "resources": [
      "resources/**/*"
    ],
    "icon": [
      "icons/32x32.png",
      "icons/128x128.png",
      "icons/128x128@2x.png",
      "icons/icon.icns",
      "icons/icon.ico"
    ]
  },
  "productName": "dataans",
  "mainBinaryName": "dataans",
  "version": "0.2.1",
  "identifier": "com.tbt.dataans",
  "plugins": {},
  "app": {
    "withGlobalTauri": true,
    "windows": [
      {
        "title": "dataans",
        "width": 1300,
        "height": 700,
        "x": 0,
        "y": 0,
        "closable": true,
        "maximized": true
      }
    ],
    "security": {
      "assetProtocol": {
        "scope": [
          "$APPCACHE/**/*",
          "$RESOURCE/**/*",
          "$APPDATA/**/*"
        ],
        "enable": true
      },
      "csp": null
    },
    "trayIcon": {
      "iconPath": "icons/icon.png",
      "iconAsTemplate": true
    }
  }
}